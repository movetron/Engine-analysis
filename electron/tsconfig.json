{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist-electron",
    "module": "CommonJS",
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES6", "ES2020"],
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "rootDir": "."
  },
  "include": ["electron/main.ts", "electron/preload.ts", "./main.ts", "./preload.ts", "./**/*.ts"],
  "exclude": ["node_modules", "../dist-electron"]
}
